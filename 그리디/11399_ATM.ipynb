   ],
   "source": [
    "n = int(input())\n",
    "arr = [int(x) for x in input().split()]\n",
    "\n",
    "arr.sort()\n",
    "dp = [0] * n\n",
    "dp[0] = arr[0]\n",
    "for i in range(1, n):\n",
    "    dp[i] = dp[i-1] + arr[i] \n",
    "    \n",
    "print(sum(dp))"
   ]
  }
