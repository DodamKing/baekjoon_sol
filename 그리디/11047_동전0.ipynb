     "output_type": "execute_result"
    }
   ],
   "source": [
    "n, k = map(int, input().split())\n",
    "won = [0] * n\n",
    "cnt = 0 # 초기화 안해주니까 백준에서 에러 나더라\n",
    "\n",
    "for i in range(n):\n",
    "    won[i] = int(input())\n",
    "    \n",
    "won.reverse()\n",
    "for i in won:\n",
    "    cnt += k // i\n",
    "    k = k % i\n",
    "    \n",
    "print(cnt)"
   ]
  }
